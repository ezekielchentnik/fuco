{
  "name": "functional-web-component",
  "version": "0.3.0",
  "description": "WebComponents with lit-html and Hooks",
  "main": "dist/functional-web-component.js",
  "module": "dist/functional-web-component.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && rollup -c",
    "test": "tsc && karma start",
    "lint": "tsc --noEmit && eslint src/** --ext .ts",
    "fmt": "npm run lint -- --fix",
    "clean": "rm -fr dist .tsbuildinfo",
    "distclean": "npm run clean && rm -fr node_modules package-lock.json",
    "prepare": "npm run clean && npm run build"
  },
  "keywords": [],
  "author": "Keisuke Watanabe",
  "license": "MIT",
  "homepage": "https://github.com/wtnbass/functional-web-component",
  "repository": "wtnbass/functional-web-component",
  "dependencies": {
    "lit-html": "1.0.0"
  },
  "devDependencies": {
    "@types/jasmine": "3.3.12",
    "@typescript-eslint/eslint-plugin": "1.7.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-plugin-prettier": "3.0.1",
    "karma": "4.1.0",
    "karma-firefox-launcher": "1.1.0",
    "karma-jasmine": "2.0.1",
    "karma-mocha-reporter": "2.2.5",
    "karma-rollup-preprocessor": "7.0.0",
    "prettier": "1.17.0",
    "rollup": "1.10.1",
    "rollup-plugin-filesize": "6.0.1",
    "rollup-plugin-node-resolve": "4.2.3",
    "rollup-plugin-terser": "4.0.4",
    "typescript": "3.4.5"
  }
}
