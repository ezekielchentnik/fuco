{
  "name": "fuco",
  "version": "1.0.0",
  "description": "Functional Component like React, but for Web Components",
  "main": "dist/fuco.js",
  "module": "dist/fuco.mjs",
  "typings": "index.d.ts",
  "files": [
    "dist",
    "core",
    "index.d.ts",
    "index.js",
    "index.js.map"
  ],
  "scripts": {
    "build": "tsc && rollup -c",
    "test": "karma start",
    "lint": "tsc --noEmit && eslint src --ext .ts",
    "fmt": "npm run lint -- --fix",
    "clean": "rm -fr dist coverage .tsbuildinfo core __tests__ index.*",
    "distclean": "npm run clean && rm -fr node_modules package-lock.json",
    "prepare": "npm run clean && npm run build"
  },
  "keywords": [
    "webcomponents",
    "lit-html",
    "hooks"
  ],
  "author": "Keisuke Watanabe",
  "license": "MIT",
  "homepage": "https://github.com/wtnbass/fuco",
  "repository": "wtnbass/fuco",
  "dependencies": {
    "lit-html": "1.1.1"
  },
  "devDependencies": {
    "@types/jasmine": "3.3.13",
    "@typescript-eslint/eslint-plugin": "1.12.0",
    "@typescript-eslint/parser": "1.12.0",
    "codecov": "3.5.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "istanbul-instrumenter-loader": "3.0.1",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-firefox-launcher": "1.1.0",
    "karma-jasmine": "2.0.1",
    "karma-webpack": "4.0.2",
    "prettier": "1.18.2",
    "puppeteer": "1.19.0",
    "rollup": "1.16.7",
    "rollup-plugin-copy": "3.0.0",
    "rollup-plugin-filesize": "6.1.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "5.1.1",
    "ts-loader": "6.0.4",
    "typescript": "3.5.3",
    "webpack": "4.35.3"
  }
}
