{
  "name": "functional-web-component",
  "version": "0.1.3",
  "description": "WebComponents with lit-html and Hooks",
  "main": "dist/functional-web-component.js",
  "module": "dist/functional-web-component.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && rollup -c",
    "clean": "rm -fr dist .cache",
    "distclean": "npm run clean && rm -fr node_modules package-lock.json",
    "lint": "eslint {src,test,demo}/**/*.ts",
    "fmt": "npm run lint -- --fix",
    "test": "karma start",
    "demo": "parcel demo/index.html",
    "prepublishOnly": "npm run lint -s && npm run build"
  },
  "keywords": [],
  "author": "Keisuke Watanabe",
  "license": "MIT",
  "homepage": "https://github.com/wtnbass/functional-web-component",
  "repository": "wtnbass/functional-web-component",
  "dependencies": {
    "lit-html": "1.0.0"
  },
  "devDependencies": {
    "@types/jasmine": "3.3.12",
    "@typescript-eslint/eslint-plugin": "1.6.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-prettier": "3.0.1",
    "karma": "4.0.1",
    "karma-firefox-launcher": "1.1.0",
    "karma-jasmine": "2.0.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-typescript": "4.0.0",
    "karma-webpack": "3.0.5",
    "prettier": "1.16.4",
    "rollup": "1.9.0",
    "rollup-plugin-terser": "4.0.4",
    "ts-loader": "5.3.3",
    "typescript": "3.4.1",
    "webpack": "4.29.6"
  }
}
